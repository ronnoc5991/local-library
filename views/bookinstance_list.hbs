<h1> {{ title }} </h1>

<ul>
    {{#each bookinstance_list}}
        <li>
            <a href={{ this.url }}>
                {{ this.book.title }} : {{ this.imprint }} -
            </a>
            <span class="text-success">
                {{ this.status }}
            </span>
            <span>
                Due: {{ this.due_back_formatted }}
            </span>
        </li>
    {{/each}}
</ul>